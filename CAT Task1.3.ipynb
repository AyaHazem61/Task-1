   "source": [
    "def split_and_join(line):\n",
    "    s1 = \"\"\n",
    "    n = len(line)\n",
    "    for i in range(0,n):\n",
    "        if line[i] == ' ' :\n",
    "            s1 += \"-\"\n",
    "        else :\n",
    "            s1 += line[i]\n",
    "    return s1"
   ]
